                                isAllvalueValid = true;
                            }
                            else {
                                csAlertCustom("Invalid! Capcha Text!");
                                isAllvalueValid = false;
                            }
                        }
                        else {
                            csAlertCustom("The mandatory (*) fields are required.");                            
                            isAllvalueValid = false;
                        };
                        if (isAllvalueValid) {
                            $.ajax({
                                type: "POST",
