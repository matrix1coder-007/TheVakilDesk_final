<script type="text/javascript">
    $(document).ready(function () {
        $('[data-toggle="tooltip"]').tooltip();
        var BASE_URL = $('body').attr('data-base-url');

        $("#Input_SearchOptionTabFlag").val(1);        

        $('.nav-tabs a').on('shown.bs.tab', function (event) {
            var activetab = $(event.target).text();
            var previoustab = $(event.relatedTarget).text();
            var activetabhref = $(event.target).attr("href");

            $(event.target).find("span").addClass("option-tabs-custom-bar");
            $(event.relatedTarget).find("span").removeClass("option-tabs-custom-bar");

            switch (activetabhref) {
                case '#Party-name':
                    $('#contentGridRequestId').attr("style", "display:block");
                    $('#Input_PartyName_CaptchaText').val('');
                    break;
                case '#Case-number':
                    $('#contentGridRequestId').attr("style", "display:block");
                    $('#Input_CaseNumber_CaptchaText').val('');
                    break;
                case '#Advocate-name':
                    $('#contentGridRequestId').attr("style", "display:block");
                    $('#Input_CounselName_CaptchaText').val('');
                    break;
                case '#Complaint-type':
                    $('#contentGridRequestId').attr("style", "display:block");
                    $('#Input_ComplaintType_CaptchaText').val('');
                    break;
                case '#Know-by-return-SMS':
                    $('#contentGridRequestId').attr("style", "display:none");
                    $('#contentGridShownId').attr("style", "display:none");
                    break;
                default:
                    alert('Oops! Something went wrong.');
            };            
        });

        $("#CaseStatusform").on("submit", function (event) {
            event.preventDefault();
            $('form').validate();
            if ($('form').valid()) {
                var activetabhref = $('ul#casestatusTabs li').find('a.active').attr("href");
                switch (activetabhref) {
                    case '#Party-name':
                        var isAllvalueValid = false;
                        if ($('#Input_PartyName_CaptchaText').val() != '') {
                            var varCapchaPartyName = $("#Input_PartyName_CaptchaText").val();
                            $("#Input_SearchOptionTabFlag").val(1);                            
                            var n = varCapchaPartyName.length;
                            if (parseInt(n) >= parseInt(6)) {
                                isAllvalueValid = true;
                            }
                            else {
                                csAlertCustom("Invalid! Capcha Text!");                                                               
                                isAllvalueValid = false;
                            }
                        }
                        else {
                            csAlertCustom("The mandatory (*) fields are required.");                                                        
                            isAllvalueValid = false;
                        };
                        if (isAllvalueValid) {
                            $.ajax({
                                type: "POST",
                                cache: false,
                                url: BASE_URL + 'CourtView/ComplaintCaseStatusInfo',
                                data: $('form').serialize(),
                                beforeSend: function () {
                                    startPreloader();
                                },
                                success: function (data, xhr, settings) {
                                    stopPreloader();
                                    $('#contentGridShownId').attr("style", "display:none");
                                    $('#viewCaseStatusList').html(data);                                    
                                },
                                error: function (xhr, textStatus, error) {
                                    stopPreloader();
                                    $('#viewCaseStatusList').html(textStatus);
                                }
                            });
                        };
                        break;
                    case '#Case-number':
                        var isAllvalueValid = false;
                        if ($('#Input_CaseNumber_CaptchaText').val() != '') {
                            var varCapchaCaseNumber = $("#Input_CaseNumber_CaptchaText").val();
                            $("#Input_SearchOptionTabFlag").val(2);
